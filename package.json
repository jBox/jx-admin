    "reselect": "^3.0.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "0.28.7",
    "del-cli": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "forever": "^0.15.3",
    "nodemon": "^1.17.3",
    "npm-install-webpack-plugin": "^4.0.4",
    "postcss-font-magician": "^2.1.1",

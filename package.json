{
  "name": "jx-admin",
  "version": "1.0.0",
  "description": "jx Admin",
  "main": "server.js",
  "scripts": {
    "clean:dist": "del-cli static/dist/**",
    "build": "npm run clean:dist && webpack --config webpack.config.prod.js",
    "dev": "npm run clean:dist && webpack",
    "dev:watch": "npm run clean:dist && webpack --watch",
    "start": "nodemon --watch src --watch app server",
    "startup": "forever start server.js",
    "shutdown": "forever stop server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "config": {
    "css": "[name]_[local]__[hash:base64:5]"
  },
  "keywords": [
    "css-modules"
  ],
  "author": "jBox",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chaos-fetch": "^1.0.0",
    "chaos-magician": "^1.0.0",
    "classnames": "^2.2.5",
    "config-vars": "^1.0.1",
    "css-modules-require-hook": "next",
    "express": "^4.14.0",
    "fs-extra": "^5.0.0",
    "jquery": "^3.3.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.3.0",
    "react-browser-router": "^2.1.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select2-wrapper": "^1.0.4-beta6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
